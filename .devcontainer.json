{
  "name": "btk-lib",
  "image": "btk-cmake",
  "extensions": [
    "eamodio.gitlens",
    "cheshirekow.cmake-format",
    "coenraads.bracket-pair-colorizer",
    "maddouri.cmake-tools-helper",
    "ms-ceintl.vscode-language-pack-ja",
    "ms-vscode.cpptools",
    "shardulm94.trailing-spaces",
    "skyapps.fish-vscode",
    "streetsidesoftware.code-spell-checker",
    "twxs.cmake",
    "vector-of-bool.cmake-tools"
  ],
  "settings": {
    "cmake.environment": {
      "BOOST_PATH": "/home/user/.linuxbrew/Cellar/boost/1.72.0/include"
    },
    "terminal.integrated.shell.linux": "/home/user/.linuxbrew/bin/fish"
  }
}